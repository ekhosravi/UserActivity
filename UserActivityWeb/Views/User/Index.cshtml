<div class="p-4" style="border-radius:20px;">
    <div class="row">
        <div class="col-6">
            <h2 class="text-primary">User List </h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Upsert" asp-controller="User" class="btn btn-primary text-white"><i class="fas fa-plus"></i> &nbsp; Create New User</a>
        </div>
    </div>
    <br />
    <br />
    <div style="background-color:#cde8e2" class="p-3 rounded">
        <table id="tblData" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr class="table-danger">
                    <th>  User Name </th>
                    <th>  Email </th>
                    <th>  Status </th>
                    <th>  Registration Date </th>
                    <th>  LastLogin Date </th>
                    <th> </th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts {
    <script src="~/js/Userr.js"></script>
}
